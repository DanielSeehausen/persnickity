<p id="notice"><%= notice %></p>

<h1>Neighborhoods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dominant cuisine</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @neighborhoods.each do |neighborhood| %>
      <tr>
        <td><%= neighborhood.name %></td>
        <td><%= link_to 'Show', neighborhood %></td>
      </tr>
    <% end %>
  </tbody>
</table>
